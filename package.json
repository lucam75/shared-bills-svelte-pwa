{
  "name": "shared-bills-svelte-pwa",
  "version": "1.0.0",
  "description": "PWA built in Svelte to keep track of shared bills using the Salesforce API",
  "main": " ",
  "scripts": {
    "build": "rollup -c",
    "build:auto": "rollup -c -w",
    "serve": "sirv dist",
    "serve:dev": "sirv dist --dev",
    "dev": "run-p build:auto serve:dev",
    "now-build": "rollup -c",
    "test": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucam75/shared-bills-svelte-pwa"
  },
  "keywords": [
    "Svelte",
    "PWA",
    "Salesforce"
  ],
  "author": "noprodev",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lucam75/shared-bills-svelte-pwa/issues"
  },
  "homepage": "https://github.com/lucam75/shared-bills-svelte-pwa#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "chokidar": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress": "^3.8.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy-assets": "^2.0.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.24.1",
    "svelte-preprocess": "^2.16.0",
    "svelte-preprocess-postcss": "^1.1.1",
    "tailwindcss": "^1.8.3"
